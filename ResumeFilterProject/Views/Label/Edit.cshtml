@model ResumeFilterProject.Models.ResumeLabel

<h2>Label Resume</h2>

<form asp-action="Edit" method="post">
    <input type="hidden" asp-for="Id" />

    <div>
        <label>Name</label>
        <input asp-for="Name" class="form-control" />
    </div>
    <div>
        <label>Contact</label>
        <input asp-for="Contact" class="form-control" />
    </div>
    <div>
        <label>Skills</label>
        <textarea asp-for="Skills" class="form-control"></textarea>
    </div>
    <div>
        <label>Experience</label>
        <textarea asp-for="Experience" class="form-control"></textarea>
    </div>
    <div>
        <label>Education</label>
        <textarea asp-for="Education" class="form-control"></textarea>
    </div>
    <div>
        <label>Projects</label>
        <textarea asp-for="Projects" class="form-control"></textarea>
    </div>

    <button type="submit" class="btn btn-primary">Save Labels</button>
</form>
